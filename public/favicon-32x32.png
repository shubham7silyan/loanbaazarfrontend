  <!-- Bank Columns -->
  <rect x="8" y="16" width="2" height="10" fill="#2a2a72"/>
  <rect x="11" y="16" width="2" height="10" fill="#2a2a72"/>
  <rect x="14" y="16" width="2" height="10" fill="#2a2a72"/>
  <rect x="17" y="16" width="2" height="10" fill="#2a2a72"/>
  <rect x="20" y="16" width="2" height="10" fill="#2a2a72"/>
  
  <!-- Bank Roof -->
  <polygon points="4,14 16,8 28,14" fill="#ffd700"/>
  
  <!-- Dollar Sign -->
  <circle cx="16" cy="20" r="4" fill="#2a2a72"/>
  <text x="16" y="24" font-family="Arial" font-size="6" font-weight="bold" text-anchor="middle" fill="#f4c542">$</text>
